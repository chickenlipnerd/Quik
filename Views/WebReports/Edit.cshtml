@model QuikServe.Models.WebReportSettings

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Edit</title>
</head>
<body>
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    
    @using (Html.BeginForm()) {
        @Html.ValidationSummary(true)
        <fieldset>
            <legend>WebReportSettings</legend>
    
            @Html.HiddenFor(model => model.WebReportSettingId)
    
            <div class="editor-label">
                @Html.LabelFor(model => model.WebReportId, "WebReports")
            </div>
            <div class="editor-field">
                @Html.DropDownList("WebReportId", String.Empty)
                @Html.ValidationMessageFor(model => model.WebReportId)
            </div>
    
            <div class="editor-label">
                @Html.LabelFor(model => model.CustomReportName)
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.CustomReportName)
                @Html.ValidationMessageFor(model => model.CustomReportName)
            </div>
    
            <div class="editor-label">
                @Html.LabelFor(model => model.DateRangeType)
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.DateRangeType)
                @Html.ValidationMessageFor(model => model.DateRangeType)
            </div>
    
            <div class="editor-label">
                @Html.LabelFor(model => model.SeedDate)
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.SeedDate)
                @Html.ValidationMessageFor(model => model.SeedDate)
            </div>
    
            <div class="editor-label">
                @Html.LabelFor(model => model.SeedIncrement)
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.SeedIncrement)
                @Html.ValidationMessageFor(model => model.SeedIncrement)
            </div>
    
            <div class="editor-label">
                @Html.LabelFor(model => model.Type)
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.Type)
                @Html.ValidationMessageFor(model => model.Type)
            </div>
    
            <div class="editor-label">
                @Html.LabelFor(model => model.ColumnDefinition)
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.ColumnDefinition)
                @Html.ValidationMessageFor(model => model.ColumnDefinition)
            </div>
    
            <p>
                <input type="submit" value="Save" />
            </p>
        </fieldset>
    }
    
    <div>
        @Html.ActionLink("Back to List", "Index")
    </div>
</body>
</html>
